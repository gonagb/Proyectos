// Declaramos varias funciones
function saluda(){
    console.log("Me llamo Goncho")
    console.log("Tengo 27 años")
    console.log("Mi correo es goncho@gonagb.com")
}

function saludaJV(){
    console.log("Me llamo Jose Vicente");
    console.log("Tengo 45 años");
    console.log("Mi correo es info@josevicentecarratala.com");
}

// Llamamos a las funciones de manera independiente
saluda();

saludaJV();